{
  "name": "template",
  "private": true,
  "version": "0.0.0",
  "description": "Webpack - The Good Parts",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "NODE_ENV=production webpack --config webpack.config.production.js",
    "lint": "eslint --ext .js,.jsx .",
    "deploy": "npm run build && npm run copy && gh-pages -d out",
